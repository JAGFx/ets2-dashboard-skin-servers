{
  "compilerOptions": {
    "paths" : {
      "@server-lib/*": ["./src/*"],
    },
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "outDir" : "dist",
    "strict": true,
	"moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strictPropertyInitialization": false,
    "types" : ["@types/jest"]
  }
}